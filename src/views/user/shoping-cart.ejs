	<script src="vendor/sweetalert/sweetalert.min.js"></script>

	<script>

		// for applying coupon
		$('#couponForm').submit((e)=>{
			e.preventDefault() ;
			
			const couponData = $('#couponForm').serialize() ;
			
			// POST request to /applyCoupon
			$.ajax({
				url : '/applyCoupon' ,
				method : 'post',
				data : couponData ,
				success : (response) => {
					if(response.status){
						toastr.success('Coupn addedd !', "Successfully", {
							closeButton: true,
							progressBar: true,
							timeOut: 3000,
						});
						// Update the cart subtotal
						$('.cart-total').text(response.cartTotal);
					}
				}
			})

		})

		// for show empty cart
		$('.cart-icon ').on('click',(e)=>{
			e.preventDefault() ;
			$.ajax({
				url : '/cart',
				method : 'get',
				success : (response) => {
					if(response.cart){
						swal('cart is empty!','add products to cart', "info");
					}
					else{
						location.href = '/cart';
					}
				}
			})
		})
		
		function changeQuantity(cartItemId, cartId, count) {
			const quantityValue = document.querySelector('.num-product').value 
			if( quantityValue > 0){
				$.ajax({
				url: '/cart/changeQuantity',
				data: {
					cartItemId,
					cartId,
					count
				},
				method: 'patch',
				success: (response) => {
					if (response.success) {
						// Update the quantity in the input field
						$(`tr[data-cart-item-id="${cartItemId}"] .num-product`).val(response.cartItem.quantity);
						
						// Update the total price for the item
						$(`tr[data-cart-item-id="${cartItemId}"] .cartItem-total`).text(response.cartItem.total);

						// Update the cart subtotal
						$('.cart-total').text(response.cartTotal);

						console.log('Quantity updated successfully.');
					} else {
						console.error('Failed to update quantity:', response.message);
					}
				},
				error: (error) => {
					console.error('Error updating quantity:', error);
				}
			});
			}
		}


		// for place order 
		$("#checkout-form").submit((e)=>{
			// for prevent default submission
			e.preventDefault() ;

			const formData = $('#checkout-form').serialize() ;

			$.ajax({
				url : '/placeOrder' ,
				method : 'post' ,
				// for pass all data inside the form by using serialize method
				data : formData ,
				success : (response)=>{
					if(response.codPayment){
						location.href = '/orders' 
					}
					else if (response.onlinePayment){
						console.log('hey razorpay is working')
						// function for create order and hendler
						razorpayPayment (response.order,response.user)
					}
					else if(response.addressError){
						console.log('address error') ;
						document.querySelector('.error-message').textContent = response.message 
					}
				}
			})
		}) ;

		function razorpayPayment (order,user) {
			console.log(order)
			const options = {
					"key": "rzp_test_HSolpJ16ypiCZg", // Enter the Key ID generated from the Dashboard
					"amount": order.amount , // Amount is in currency subunits. Default currency is INR. Hence, 50000 refers to 50000 paise
					"currency": "INR",
					"name": "Sayanth",
					"description": "Test Transaction",
					"image": "https://example.com/your_logo",
					"order_id": order.id , //This is a sample Order ID. Pass the `id` obtained in the response of Step 1
					"handler": function (response){
						// alert(response.razorpay_payment_id);
						// alert(response.razorpay_order_id);
						// alert(response.razorpay_signature)

						console.log(response) ;
						verifyPayment(response,order) ;
					},
					"prefill": {
						"name": user.name ,
						"email": user.emailId,
						"contact": order.amount
					},
					"notes": {
						"address": "Razorpay Corporate Office"
					},
					"theme": {
						"color": "#3399cc"
					}
				};
				const rzp1 = new Razorpay(options);
				rzp1.open();
				rzp1.on('payment.failed', function (response){
						alert(response.error.code);
						alert(response.error.description);
						alert(response.error.source);
						alert(response.error.step);
						alert(response.error.reason);
						alert(response.error.metadata.order_id);
						alert(response.error.metadata.payment_id);
				});
		}

		function verifyPayment(payment,order) {
			$.ajax({
				url : '/verify-payment' , 
				method : 'post' ,
				data : {
					payment ,
					order
				},
				success : (response) => {
					if(response.status){
						location.href = '/orders'
					}
					else{
						alert('payment failed') ;
					}
				}
			})
		}
	</script>
	

